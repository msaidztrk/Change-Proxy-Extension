{
    "manifest_version": 3,
    "name": "Dynamic Proxy Switcher",
    "version": "1.0",
    "icons": {
        "16": "/icons/ico.png",
        "48": "/icons/ico.png",
        "128":"/icons/ico.png"
    },
    "permissions": [
        "identity",
        "storage",
        "proxy",
        "webRequest",
        "privacy",
        "webRequestAuthProvider",
        "cookies"
        
    ],
    "host_permissions": ["<all_urls>"] ,
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js" ,
         "type": "module"
    }
}
